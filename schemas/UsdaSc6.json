    "properties": {
        "@context": {
            "type": "array"
        },
        "type": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "array"
                }
            ]
        },
        "serialNumber": {
            "$comment": "{\"term\": \"serialNumber\", \"@id\": \"https://w3id.org/traceability#serialNumber\", \"@type\": \"https://schema.org/identifier\"}",
            "title": "Serial Number",
            "description": "Serial number of the form.",
            "type": "string"
        },
        "customsEntryNumber": {
            "$comment": "{\"term\": \"customsEntryNumber\", \"@id\": \"https://w3id.org/traceability#customsEntryNumber\", \"@type\": \"https://schema.org/identifier?v3\"}",
            "title": "Customs Service Entry Number",
            "description": "U.S. Customs Service Entry Number",
            "type": "string"
        },
        "tariffCodeNumber": {
            "$comment": "{\"term\": \"tariffCodeNumber\", \"@id\": \"https://w3id.org/traceability#tariffCodeNumber\", \"@type\": \"https://schema.org/identifier?v2\"}",
